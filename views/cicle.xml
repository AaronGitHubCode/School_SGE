<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="action_cicle" model="ir.actions.act_window">
        <field name="name">Cicle</field>
        <field name="res_model">school.cicle</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="model_name_view_tree" model="ir.ui.view">
        <field name="name">Cicle</field>
        <field name="model">school.cicle</field>
        <field name="arch" type="xml">
            <tree string="Cicles">
                <field name="name" />
                <field name="module_relation" />
            </tree>
        </field>
    </record>

    <menuitem id="cicle_section" name="Cicles" parent="menu_root" action="action_cicle" />
</odoo>
